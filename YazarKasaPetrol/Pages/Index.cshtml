@page
@using YazarKasaPetrol.Models.Interfaces;
@using Models;
@using YazarKasaPetrol.Controller;
@model YazarKasaPetrol.Pages.ApiTableModel
@{
    Func<List<string>, string> stringConverter = (s) =>
    {
        string returnString = "";

        s.ForEach(x =>
        {
            returnString += x;
        });

        return returnString;
    };
}

@{
    if (AuthType.TheCurrentAuth != null && AuthType.TheCurrentAuth.IsSuperAdmin)
    {
        <div class="row g-0 align-items-center">
            <div class="p-1">
                <a class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#addCashModal">
                    <i class="bx bx-plus me-1"></i> Yeni firma ekle
                </a>
                <a class="btn btn-primary" id="invoiceModalOpener" data-bs-toggle="modal" data-bs-target="#searchInvoiceModal">
                    <i class="bx bx-search me-1"></i> Plaka ara
                </a>
            </div>
            <div class="p-3 row">
                <div class="col-md-5">
                    <div class="card p-2">
                        <div class="table-responsive">
                            <table class="table project-list-table table-nowrap align-middle table-borderless">
                                <thead>
                                    <tr>
                                        <th scope="col">Benzin istasyonu adı</th>
                                        <th scope="col">İşlemler</th>
                                        <th scope="col" style="width: 50px;">Ekü</th>
                                    </tr>
                                </thead>
                                <tbody id="tableElements">
                                    @{
                                        List<Models.SuperAdmin> superAdmin = ((CashContent)Retriever.RetrieveTables(Utilities.PATH)).DataContent.Where(x => x.TaxNumber == AuthType.TheCurrentAuth.UserCredentialsForInvoice.TaxNumber).ToList();

                                        List<IModelElement> allElements = new()
                                        {
                                            superAdmin[0]
                                        };
                                        
                                        List<Admin> theSubAdmins = ((CashContent)Retriever.RetrieveTables(Utilities.PATH1)).DataContent_1.Where(x => x.AdminTaxNumber == AuthType.TheCurrentAuth.UserCredentialsForInvoice.TaxNumber).ToList();
                                        List<AdminModel> groupedAdminModels = theSubAdmins.Select(admin => admin.AdminModel).ToList();
                                        allElements.AddRange(groupedAdminModels);

                                        for (int i = 0; i < allElements.Count; i++)
                                        {
                                            <tr id="adminPanelContent_@i">
                                                <td>
                                                    <a class="mb-0 taxNumberLink text-dark" id="taxNumber_@i" onclick="getTheTaxNumber('@Url.Page("index", "cashzerosforinvoice")', '@Url.Page("index", "taxnumberinvoicemaker")', '@allElements[i].TaxNumber', @i)">@allElements[i].GasStationName[0]</a>
                                                </td>
                                                <td>
                                                    <ul class="list-inline mb-0">
                                                        @{
                                                            if (i != 0)
                                                            {
                                                                <li class="list-inline-item">
                                                                    <a href="javascript:void(0);" data-bs-toggle="modal" data-bs-placement="top" data-bs-target="#updateOtherCashModal" onclick="updateApiKey_2('@allElements[i].TaxNumber', '@Url.Page("index", "cashzerosforinvoice")')" title="Update the cash info." class="px-2 text-secondary">
                                                                        <i class='bx bxs-comment-edit font-size-1'></i>
                                                                    </a>
                                                                </li>
                                                            }
                                                            else
                                                            {
                                                                <li class="list-inline-item">
                                                                    <a href="javascript:void(0);" data-bs-toggle="modal" data-bs-placement="top" data-bs-target="#updateCashModal" onclick="updateApiKey('@allElements[i].TaxNumber', '@Url.Page("index", "cashzerosforinvoice")')" title="Update the cash info." class="px-2 text-secondary">
                                                                        <i class='bx bxs-comment-edit font-size-1'></i>
                                                                    </a>
                                                                </li>
                                                            }
                                                        }

                                                        <li class="list-inline-item">
                                                            <a asp-page="/GasPrices" asp-route-taxnumber="@allElements[i].TaxNumber" title="Benzin fiyatlarını düzenler" class="px-2 text-warning">
                                                                <i class='bx bx-money font-size-1'></i>
                                                            </a>
                                                        </li>
                                                        <li class="list-inline-item">
                                                            <a asp-page="/GasStationInvoices" asp-route-taxnumber="@allElements[i].TaxNumber" title="Bütün firmanın faturalarına bakar." class="px-2 text-info">
                                                                <i class='bx bx-search-alt font-size-1'></i>
                                                            </a>
                                                        </li>
                                                        <li class="list-inline-item">
                                                            <a asp-page="/ZReportsSystem" asp-route-taxnumber="@allElements[i].TaxNumber" title="Bütün firmanın z raporu no'larına bakar." class="px-2 text-danger">
                                                                <i class='bx bx-search-alt font-size-1'></i>
                                                            </a>
                                                        </li>
                                                    </ul>
                                                </td>
                                                <td>
                                                    <ul class="list-inline mb-0">
                                                        <li class="list-inline-item">
                                                            <a href="javascript:void(0);" data-bs-toggle="modal" data-bs-target="#ekuNumberAddingModal" onclick="ekuAddPlusOneButton('@allElements[i].TaxNumber', '@Url.Page("index", "ekuaddoneplus")')" class="px-2 text-secondary">
